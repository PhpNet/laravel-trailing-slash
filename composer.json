{
    "name": "phpnet/laravel-trailing-slash",
    "description": "Adds redirection with trailing slash in Laravel.",
    "keywords": ["redirect", "laravel", "php"],
    "license": "MIT",
    "homepage": "https://github.com/PhpNet/phpnet-laravel-trailing-slash/",
    "authors": [
        {
            "name": "Serg Serdyuk",
            "email": "sserdyuk@webakula.ua"
        }
    ],
    "require": {
        "illuminate/routing": "5.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "LaravelTrailingSlash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTrailingSlash\\RoutingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}